                // Variables se data uthana
                string filePath = Dts.Variables["User::FilePath"].Value.ToString();
                string errorDesc = Dts.Variables["System::ErrorDescription"].Value.ToString();

                // Gmail SMTP ki configuration
                var smtpClient = new System.Net.Mail.SmtpClient("smtp.gmail.com")
                {
                    Port = 587,
                    Credentials = new System.Net.NetworkCredential("usman.pk7103@gmail.com", "atkrspzkyyhqhcpa"),
                    EnableSsl = true,
                };

                // Email ka subject aur body tayyar karna
                string subject = "SSIS Critical Alert: File Processing Failed";
                string body = "Hello,\n\nAn error occurred in the SSIS package.\n\n" +
                              "Failed File: " + filePath + "\n" +
                              "Error Details: " + errorDesc + "\n\n" +
                              "Please check the error folder and logs.";

                // Email bhejna
                smtpClient.Send("usman.pk7103@gmail.com", "urdunovelbank01@gmail.com", subject, body);

                Dts.TaskResult = (int)ScriptResults.Success;
            }
            catch (Exception ex)
            {
                // Agar email send na ho sake toh error log karna
                Dts.Events.FireError(0, "Email Script", ex.Message, string.Empty, 0);
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
        }

